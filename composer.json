{
    "name": "sarowar/laravel_slug_generator",
    "description": "A unique slug generator for laravel application",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sarowar\\LaravelSlugGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "S.M. Sarowar"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sarowar\\LaravelSlugGenerator\\LaravelSlugServiceProvider"
            ],
            "aliases": {
                "laravelSlug": "Sarowar\\LaravelSlugGenerator\\Facade"
            }
        }
    }
}
